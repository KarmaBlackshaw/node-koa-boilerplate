{
  "name": "node-koa-boilerplate",
  "version": "2.0.7",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "make:resource": "plop --plopfile ./generator/resource/index.js",
    "make:job": "plop --plopfile ./generator/job/index.js",
    "generate:key": "node ./bin/key-generator.js",
    "generate:env": "node ./bin/env-generator.js",
    "generate:port": "node ./bin/get-free-port.js",
    "release": "release-it",
    "prepare": "husky install"
  },
  "env": {
    "node": true
  },
  "keywords": [
    "boilerplate",
    "koa",
    "javascript",
    "redis",
    "knex",
    "mysql",
    "socket.io",
    "database",
    "utilities",
    "utilities",
    "logger",
    "env-validate",
    "node"
  ],
  "author": "Ernie Jeash",
  "license": "ISC",
  "dependencies": {
    "@jeash/knex-meta": "^1.3.2",
    "@koa/cors": "2.2.2",
    "axios": "^0.27.2",
    "bcryptjs": "2.4.3",
    "bluebird": "3.5.2",
    "chalk": "^4.1.2",
    "destr": "^1.2.2",
    "dotenv": "6.1.0",
    "glob": "^8.1.0",
    "joi": "17.3.0",
    "jsonwebtoken": "8.3.0",
    "knex": "^2.4.2",
    "koa": "2.5.3",
    "koa-bodyparser": "4.2.1",
    "koa-compress": "3.0.0",
    "koa-morgan": "^1.0.1",
    "koa-mount": "^4.0.0",
    "koa-router": "7.4.0",
    "koa-static": "^5.0.0",
    "koa2-formidable": "^1.0.3",
    "lodash": "^4.17.21",
    "module-alias": "2.2.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "mysql": "2.16.0",
    "redis": "^4.6.4",
    "simple-node-logger": "21.8.12",
    "socket.io": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "eslint": "^8.33.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "4.0.1",
    "faker": "^6.6.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "nodemon": "^2.0.20",
    "plop": "^3.1.2",
    "pluralize": "^8.0.0",
    "release-it": "^15.6.0"
  },
  "lint-staged": {
    "*": "npm run test"
  }
